{
	"main": "dist/_worker.js/index.js",
	"name": "personal-site",
  "compatibility_date": "2026-02-22",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
  "routes": [
    {
      "pattern": "odza.dev/*",
      "custom_domain": true
    }
  ],
	"observability": {
    "enabled": true
  }
}